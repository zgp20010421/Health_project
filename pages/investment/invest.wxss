.investBox{
    background:#f8f8f8;
    height:100%;
}
.invest_top{
    width:100%;
    height:58px;
    position:relative;
    background:#fff;
    padding-top:10px;
}
.cutLine{
   margin-left:10px;
   position:absolute;
   left:0;
   right:0;
   margin:auto;
   padding-top:12px;
}
.invest_first{
    width:48%;
    float:left;
    text-align:center;
}
.invest_second{
    width:48%;
    float:right;
    text-align:center;
}

/**--------------------------投资页面列表样式----------------------------**/
.scrollList{
    margin-top:10px;
}
.index_list{
    height:108px;
    padding:0 8px;
    background:#fff;
    margin-bottom:10px;
}
.index_listTit{
    width:100%;
    height:24px;
    padding-top:10px;
    border-bottom:1px solid #ccc;
}
.indexList_pic1{
    float:left;
}
.indexList_tit1{
    float:left;
    font-size:12px;
    width:150px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow:hidden;
}
.indexList_pay{
    float:left;
    margin-left:20px;
}
.indexList_paystyle{
    font-size:12px;
    color:#f4bc12;
    float:right;
}
.index_listcontent{
    height:64px;
    display:flex;
    -webkit-justify-content:space-around;
    justify-content:space-around;
    padding-top:10px;
}
.index_listcontent view{
    text-align:center;
}
.income{
    font-size:32px;
    color:#ec5e3f;
}
.index_project{
    padding-top:10px;
}
.indexFont_bottom{
    font-size:12px;
    color:#999;
}
.index_font{
    font-size:16px;
}